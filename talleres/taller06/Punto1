import numpy as np
 
class ArrayList:

     DEFAULT = 10
 
    def __init__(self):

             self.__size = 0

             self.__elements = np.array(DEFAULT)
 
    def size(self):

             return self.__size
 
    def get(self, index):
 
    def add(self, o):

             __elements[__size] = o

             __size++
 
    def addInIndex(self, index, o):
 
arr = new ArrayList()

 arr.add(23)

 print (arr)
